{
  "name": "Octopeer Github Data Collector",
  "manifest_version": 2,
  "version": "0.3.0",
  "description": "This extension collects data while the user reviews Pull Requests.",
  "author": "Rubber Duck Debuggers",

  "icons": {
    "16": "icon_large.png",
    "19": "icon_large_green.png",
    "48": "icon_large.png",
    "128": "icon_large.png"
  },

  "browser_action": {
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab", "storage", "tabs", "https://github.com/"
  ],
  "content_scripts": [ {
    "js": [ "jquery.min.js", "main.js", "materialize.min.js" ],
    "matches": [ "https://github.com/*" ],
    "run_at": "document_start"
  } ]
}
