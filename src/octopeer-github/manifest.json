{
  "name": "Octopeer Github Data Collector",
  "manifest_version": 2,
  "version": "0.1",
  "description": "This extension collects data while the user reviews Pull Requests.",
  "permissions": [
    "activeTab", "tabs", "https://github.com/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["inject.js"]
    }
  ]
}
